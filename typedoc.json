{
  "entryPoints": ["src/index.ts"],
  "out": "docs",
  "theme": "default",
  "name": "result-ts",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "readme": "README.md",
  "includeVersion": true,
  "sort": ["source-order"],
  "validation": {
    "invalidLink": true,
    "notExported": true
  },
  "categoryOrder": ["Core", "Functional Operations", "Validation", "Utilities"],
  "cleanOutputDir": true,
  "treatWarningsAsErrors": false,
  "logLevel": "Info",
  "skipErrorChecking": false
}
