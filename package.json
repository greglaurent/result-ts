{
  "name": "result-ts",
  "version": "0.2.0",
  "description": "Result TS: The type safe result library for Typescript.",
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "dev": "tsc --watch"
  },
  "keywords": [
    "result",
    "rust",
    "error-handling",
    "typescript",
    "zod",
    "validation",
    "functional",
    "modular",
    "type-safe"
  ],
  "author": "Gregory Laurent",
  "license": "MIT",
  "packageManager": "pnpm@10.12.1",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.js"
    },
    "./validation": {
      "types": "./dist/validation.d.ts",
      "import": "./dist/validation.js",
      "require": "./dist/validation.js"
    },
    "./base": {
      "types": "./dist/base.d.ts",
      "import": "./dist/base.js",
      "require": "./dist/base.js"
    }
  },
  "peerDependencies": {
    "zod": "^3.25.67"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "devDependencies": {
    "zod": "^3.25.67",
    "typescript": "^5.8.3"
  }
}
